public class Main {
    public static void main(String[] args) {
        int num = 10;

        if (num > 10) {
            System.out.println("10보다 큽니다");
        } else if (num == 10) {
            System.out.println("10과 같습니다");
        } else {
            System.out.println("10보다 작습니다");
        }
    }
}